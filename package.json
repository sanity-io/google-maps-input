{
  "name": "@sanity/google-maps-input",
  "version": "4.0.1",
  "description": "Sanity plugin providing input handlers for geo-related input types using Google Maps",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "google-maps-input",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/sanity-io/google-maps-input#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/google-maps-input/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/google-maps-input.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "node": {
        "module": "./dist/index.esm.js",
        "import": "./dist/index.cjs.mjs"
      },
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sanity/icons": "^2.0.0",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^2.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@sanity/pkg-utils": "^2.4.10",
    "@sanity/plugin-kit": "^3.1.10",
    "@sanity/semantic-release-preset": "^4.1.7",
    "@types/google.maps": "^3.55.7",
    "@types/react": "^18.2.75",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all2": "^5.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "sanity": "^3.37.2",
    "semantic-release": "^22.0.12",
    "styled-components": "^6.1.8",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3.19.0",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=14"
  },
  "alias": {
    "google": {
      "global": "google"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
